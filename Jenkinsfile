pipeline {
  agent any
  environment{
     BRANCH = "${env.BRANCH_NAME}"
  }
  stages{
        stage('Build Docker Image'){
            steps{
                sh '''
                oldSHC=afkjsdkj1232nkjsd
                SHC=afkjsdkj1232nkjsd
                if [ $SHC == $oldSHC ]
                then
                echo "True"
                else
                echo "False"
                fi  
             '''   
            }
        }
   }  
}
